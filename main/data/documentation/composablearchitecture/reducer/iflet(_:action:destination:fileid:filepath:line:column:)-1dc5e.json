pe":"text"},{"type":"codeVoice","code":"ifLet"},{"text":" operator:","type":"text"}]},{"type":"codeListing","syntax":"swift","code":["@Reducer","struct Parent {","  @ObservableState","  struct State {","    @Presents var child: Child.State?","    \/\/ ...","  }","  enum Action {","    case child(PresentationAction<Child.Action>)","    \/\/ ...","  }","","  var body: some ReducerOf<Self> {","    Reduce { state, action in","      \/\/ Core logic for parent feature","    }","    .ifLet(\\.$child, action: \\.child) {","      Child()","    }","  }","}"]},{"type":"paragraph","inlineContent":[{"text":"The ","type":"text"},{"code":"ifLet","type":"codeVoice"},{"text":" operator does a number of things to make integrating parent and child features","type":"text"},{"text":" ","type":"text"},{"text":"ergonomic and enforce correctness:","type":"text"}]},{"type":"unorderedList","items":[{"content":[{"inlineContent":[{"type":"text","text":"It forces a specific order of operations for the child and parent features:"}],"type":"paragraph"},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"When a "},{"type":"reference","isActive":true,"identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/PresentationAction\/dismiss"},{"type":"text","text":" action is sent, it runs the parent feature"},{"type":"text","text":" "},{"type":"text","text":"before the child state is "},{"type":"codeVoice","code":"nil"},{"type":"text","text":"’d out. This gives the parent feature an opportunity to"},{"type":"text","text":" "},{"type":"text","text":"inspect the child state one last time before the state is cleared."}]}]},{"content":[{"type":"paragraph","inlineContent":[{"text":"When a ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/PresentationAction\/presented(_:)"},{"text":" action is sent it runs the","type":"text"},{"text":" ","type"